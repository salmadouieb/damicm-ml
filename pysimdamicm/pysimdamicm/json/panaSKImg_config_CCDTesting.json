            "use_overscan"      : false,
            "id_skip_reference" : 1,
            "id_skip_start"     : 2,
            "id_skip_end"       : -1,
            "skip_step"         : 20,
            "kcl_threshold"     : 3.2,
            "kcl_n_sig"         : 8,
            "display"           : true
        },
        "FFTNoisePlot":
        {
        },
        "RNvsNskipsPlot":
        {
            "n_skips_per_block" : -75,
            "is_blank"          : false
        },
        "ClusterFinder":
        {
            "spr_threshold"        : {"U": [null,8,10], "L":[null,8,10]},
            "spr_image"            : "mean_compressed_pedestal_subtracted_e",
