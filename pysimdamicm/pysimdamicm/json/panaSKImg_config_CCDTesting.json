            "col_start"       : 1,
            "col_end"         : 10,
            "subtract_median" : true,
            "n_exp"           : 1,
            "fit_options"     : "QSMER"
        },
        "CalibrationProcess":
        {
            "image" : "mean_compressed_pedestal_subtracted",
            "gain"  : {"U":5.0, "L":5.0}
        },
        "FitCalibrationConstant":
        {
