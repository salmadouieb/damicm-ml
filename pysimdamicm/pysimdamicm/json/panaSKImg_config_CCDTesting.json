            "x_max"             : 60
        },
        "FitDarkCurrentPerCol":
        {
            "image"             : "mean_compressed_pedestal_subtracted",
            "col_start"         : 0,
            "col_end"           : -1,
            "col_step"          : 10,
            "rows_to_mask"      : {"L":[], "U":[]},
            "cols_to_mask"      : {"L":[0,1], "U":[[638,639]]},
            "calibration"       : 10.0,
            "n_peaks"           : 2,
