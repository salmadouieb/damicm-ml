            "N_row_pre"         : 3,
            "N_row_post"        : 10,
            "mask_cluster_column": false
        },
        "ApplySelectionCuts":
        {
            "cols"  : [10,141,42,143,144,145,146,147,148,149,150],
            "rows"  : [1,2,3,149,150]
        },
        "GaussianFitProcess":
        {
            "image"           : "mean_compressed",
            "skip_rows"       : [],
            "in_trim_mean"    : true,
            "n_sigma"         : 10
        }
    }
